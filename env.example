# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key-here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_CLIENT_SECRET=your-google-client-secret

# App Configuration
EXPO_PUBLIC_APP_NAME=FamilyDash
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_ENVIRONMENT=development

# Widget Configuration
EXPO_PUBLIC_WIDGET_ENABLED=true
EXPO_PUBLIC_WIDGET_UPDATE_INTERVAL=300000

# Notification Configuration
EXPO_PUBLIC_NOTIFICATION_ENABLED=true
EXPO_PUBLIC_NOTIFICATION_SOUND_ENABLED=true

# Analytics Configuration
EXPO_PUBLIC_ANALYTICS_ENABLED=false
EXPO_PUBLIC_ANALYTICS_TRACKING_ID=your-tracking-id

# Debug Configuration
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=info

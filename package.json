{
  "name": "familydash",
  "version": "1.4.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "secrets:scan": "gitleaks detect -v --no-git -c .gitleaks.toml || echo \"⚠️ Gitleaks not installed. Install from: https://github.com/zricethezav/gitleaks\"",
    "secrets:scan-staged": "gitleaks protect --staged -v -c .gitleaks.toml",
    "secrets:scan-history": "gitleaks detect -v -c .gitleaks.toml"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@sentry/react-native": "~7.2.0",
    "expo": "54.0.13",
    "expo-av": "~16.0.7",
    "expo-barcode-scanner": "^13.0.1",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "^15.0.7",
    "expo-device": "~8.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-localization": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "^0.32.12",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-video": "^3.0.11",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "immer": "^10.1.3",
    "mitt": "^3.0.1",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "^54.0.12",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
